@using Octokit
@model List<PullRequest>
@{
    ViewData["Title"] = "践识审核";
}
<div class="row">
    <div class="col-auto">
        <table class="table table-sm table-striped table-responsive table-bordered">
            <thead>
                <tr>
                    <th>标题</th>
                    <th>作者</th>
                    <th>提交时间</th>
                    <th>操作</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var pr in Model)
                {
                    <tr>
                        <td>
                            @pr.Title
                            <a class="text-info" href="https://github.com/TechViewsTeam/Practknow/pull/@pr.Number">
                                [#@pr.Number]
                            </a>
                        </td>
                        <td>@pr.User.Login</td>
                        <td>@pr.CreatedAt.ToString("yyyy-MM-dd HH:mm")</td>
                        <td>
                            <a href="@pr.HtmlUrl" target="_blank">
                                审核
                            </a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

@model Guandian.Data.Entity.News

@{
    ViewData["Title"] = "删除";
}

<div class="container">
    <h4>是否确定删除?</h4>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <a asp-action="Index">返回列表</a>
        <input type="submit" value="删除" class="btn btn-outline-danger" />
    </form>

    <dl class="dl-horizontal mt-2">
        <dt>
        </dt>
        <dd>
            <h4>
                @Html.DisplayFor(model => model.Title)
                <img src="@Model.ThumbnailUrl" alt="" height="120" />
            </h4>
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Content)
        </dt>
        <dd>
            @Html.Raw(Model.Content)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Url)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Url)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Provider)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Provider)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.CreatedTime)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CreatedTime)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.UpdatedTime)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.UpdatedTime)
        </dd>

    </dl>

</div>
